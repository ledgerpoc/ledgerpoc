{
  "_version": "1.81.0",
  "sap.app": {
    "id": "com.mbec.ledgerpoc.accounts",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.20.1",
      "toolsId": "5fbbca8e-e926-41fc-af16-23a60ca52def"
    },
    "dataSources": {
      "mainService": {
        "uri": "/LedgerService/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "accounts-manage": {
          "semanticObject": "accounts",
          "action": "manage",
          "title": "{{accounts-manage.flpTitle}}",
          "subTitle": "{{accounts-manage.flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.144.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.mbec.ledgerpoc.accounts.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "AccountsList",
          "target": "AccountsList"
        },
        {
          "pattern": "Accounts({key}):?query:",
          "name": "AccountsObjectPage",
          "target": "AccountsObjectPage"
        }
      ],
      "targets": {
        "AccountsList": {
          "type": "Component",
          "id": "AccountsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Accounts",
              "variantManagement": "Page",
              "navigation": {
                "Accounts": {
                  "detail": {
                    "route": "AccountsObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  },
                  "actions": {
                    "loadBalances": {
                      "press": ".extension.com.mbec.ledgerpoc.accounts.ext.controller.AccountsListExt.UploadBalances",
                      "visible": true,
                      "enabled": true,
                      "requiresSelection": false,
                      "text": "{i18n>loadBalances}"
                    },
                    "uploadTransactions": {
                      "press": ".extension.com.mbec.ledgerpoc.accounts.ext.controller.AccountsListExt.UploadTransactions",
                      "visible": true,
                      "enabled": true,
                      "requiresSelection": false,
                      "text": "{i18n>uploadTransactions}"
                    }
                  }
                }
              }
            }
          }
        },
        "AccountsObjectPage": {
          "type": "Component",
          "id": "AccountsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Accounts"
            }
          }
        }
      }
    },
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {
          "sap.fe.templates.ListReport.ListReportController": {
            "controllerName": "com.mbec.ledgerpoc.accounts.ext.controller.AccountsListExt"
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.fe": {
    "app": {
      "enableLazyLoading": true
    }
  }
}
